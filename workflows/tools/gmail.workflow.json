{
  "name": "so.tool.gmail",
  "nodes": [
    {
      "id": "manual",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    },
    {
      "id": "normalize",
      "name": "Normalize Input",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        420,
        300
      ],
      "parameters": {
        "language": "JavaScript",
        "mode": "runOnceForAllItems",
        "jsCode": "return items.map((item) => {\n  const input = item.json || {};\n  const tool = input.tool || {};\n  const params = input.params ?? tool.params ?? {};\n  const operation = input.operation ?? tool.operation ?? params.action ?? input.action;\n  return {\n    json: {\n      provider: 'gmail',\n      operation,\n      params\n    }\n  };\n});\n"
      }
    },
    {
      "id": "dispatch",
      "name": "Dispatch Operation",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        660,
        300
      ],
      "parameters": {
        "language": "JavaScript",
        "mode": "runOnceForAllItems",
        "numberOutputs": 4,
        "jsCode": "const routes = {\n  \"message.get\": 0,\n  \"message.getMany\": 1,\n  \"message.send\": 2,\n  \"sampleFetch\": 3\n};\nconst outputs = Array.from({ length: 4 }, () => []);\nfor (const item of items) {\n  const op = item.json?.operation;\n  const index = routes[op];\n  if (index !== undefined) {\n    outputs[index].push(item);\n  }\n}\nreturn outputs;\n"
      }
    },
    {
      "id": "message-get",
      "name": "message.get",
      "type": "n8n-nodes-base.gmail",
      "typeVersion": 1,
      "position": [
        920,
        150
      ],
      "parameters": {
        "resource": "message",
        "operation": "get",
        "additionalFields": {
          "format": "={{$json.params.format}}"
        },
        "messageId": "={{$json.params.messageId}}"
      }
    },
    {
      "id": "message-getMany",
      "name": "message.getMany",
      "type": "n8n-nodes-base.gmail",
      "typeVersion": 1,
      "position": [
        920,
        300
      ],
      "parameters": {
        "resource": "message",
        "operation": "getMany",
        "additionalFields": {
          "includeSpamTrash": "={{$json.params.includeSpamTrash}}",
          "limit": "={{$json.params.limit}}",
          "query": "={{$json.params.query}}"
        }
      }
    },
    {
      "id": "message-send",
      "name": "message.send",
      "type": "n8n-nodes-base.gmail",
      "typeVersion": 1,
      "position": [
        920,
        450
      ],
      "parameters": {
        "resource": "message",
        "operation": "send",
        "additionalFields": {
          "attachments": "={{$json.params.attachments}}",
          "html": "={{$json.params.html}}",
          "text": "={{$json.params.text}}",
          "threadId": "={{$json.params.threadId}}"
        },
        "subject": "={{$json.params.subject}}",
        "to": "={{$json.params.to}}"
      }
    },
    {
      "id": "sample-fetch",
      "name": "sampleFetch",
      "typeVersion": 1,
      "position": [
        920,
        600
      ],
      "type": "n8n-nodes-base.gmail",
      "parameters": {
        "resource": "message",
        "operation": "getMany",
        "additionalFields": {
          "includeSpamTrash": "={{$json.params.includeSpamTrash}}",
          "limit": "={{$json.params.limit}}",
          "query": "={{$json.params.query}}"
        }
      }
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "Normalize Input",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Input": {
      "main": [
        [
          {
            "node": "Dispatch Operation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Dispatch Operation": {
      "main": [
        {
          "node": "message.get",
          "type": "main",
          "index": 0
        },
        {
          "node": "message.getMany",
          "type": "main",
          "index": 0
        },
        {
          "node": "message.send",
          "type": "main",
          "index": 0
        },
        {
          "node": "sampleFetch",
          "type": "main",
          "index": 0
        }
      ]
    },
    "message.get": {
      "main": []
    },
    "message.getMany": {
      "main": []
    },
    "message.send": {
      "main": []
    },
    "sampleFetch": {
      "main": []
    }
  },
  "settings": {},
