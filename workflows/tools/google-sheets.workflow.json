          "index": 0
        },
        {
          "node": "spreadsheet.create",
          "type": "main",
          "index": 0
        },
        {
          "node": "sampleFetch",
          "type": "main",
          "index": 0
        }
      ]
    },
    "sheet.append": {
      "main": []
    },
    "sheet.read": {
      "main": []
    },
    "sheet.update": {
      "main": []
    },
    "spreadsheet.create": {
      "main": []
    },
    "sampleFetch": {
      "main": []
    }
  },
  "settings": {},
  "pinData": {}
}
