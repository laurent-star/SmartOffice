{
  "name": "openai tool workflow",
  "nodes": [
    {
      "id": "manual",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    },
    {
      "id": "normalize",
      "name": "Normalize Input",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        420,
        300
      ],
      "parameters": {
        "jsCode": "return items.map((item) => {\n  const input = item.json || {};\n  const tool = input.tool || {};\n  const params = input.params ?? tool.params ?? {};\n  const operation = input.operation ?? tool.operation ?? params.action ?? input.action;\n  if (!params.text) {\n    const messages = params.messages?.values ?? params.messages;\n    if (Array.isArray(messages)) {\n      params.text = messages.map((m) => m.content ?? '').join('\\n');\n    }\n  }\n  if (!params.text && params.prompt) params.text = params.prompt;\n  if (!params.text && params.input) params.text = params.input;\n  return {\n    json: {\n      provider: 'openai',\n      operation,\n      params\n    }\n  };\n});\n"
      }
    },
    {
      "id": "text-generateChatCompletion",
      "name": "text.generateChatCompletion",
      "type": "n8n-nodes-langchain.openai",
      "typeVersion": 1,
      "position": [
        920,
        150
      ],
      "parameters": {
        "resource": "text",
        "operation": "generateChatCompletion",
        "text": "={{$json.params.text}}",
        "additionalFields": {}
      }
    },
    {
      "id": "text-classifyTextForViolations",
      "name": "text.classifyTextForViolations",
      "type": "n8n-nodes-langchain.openai",
      "typeVersion": 1,
      "position": [
        920,
        300
      ],
      "parameters": {
        "resource": "text",
        "operation": "classifyTextForViolations",
        "text": "={{$json.params.text}}",
        "additionalFields": {}
      }
    },
    {
      "id": "text-generateModelResponse",
      "name": "text.generateModelResponse",
      "type": "n8n-nodes-langchain.openai",
      "typeVersion": 1,
      "position": [
        920,
        450
      ],
      "parameters": {
        "resource": "text",
        "operation": "generateModelResponse",
        "text": "={{$json.params.text}}",
        "additionalFields": {}
      }
    },
    {
      "id": "sample-fetch",
      "name": "sampleFetch",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        920,
        600
      ],
      "parameters": {
        "jsCode": "return items.map((item) => {\n  const input = item.json || {};\n  return {\n    json: {\n      ok: true,\n      data: { sample: true, provider: 'openai', operation: 'sampleFetch', params: input.params || {} },\n      error: null\n    }\n  };\n});\n"
      }
    },
    {
      "id": "dispatch",
      "name": "Dispatch Operation",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        660,
        300
      ],
      "parameters": {
        "numberOutputs": 4,
        "jsCode": "const routes = {\n  \"text.generateChatCompletion\": 0,\n  \"text.classifyTextForViolations\": 1,\n  \"text.generateModelResponse\": 2,\n  \"sampleFetch\": 3\n};\nconst outputs = Array.from({ length: 4 }, () => []);\nfor (const item of items) {\n  const op = item.json?.operation;\n  const index = routes[op];\n  if (index !== undefined) {\n    outputs[index].push(item);\n  }\n}\nreturn outputs;\n"
      }
    }
  ],
  "connections": {
