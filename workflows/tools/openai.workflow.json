{
  "name": "tool-openai",
  "nodes": [
    {
      "id": "node-trigger",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    },
    {
      "id": "node-normalize",
      "name": "Normalize Input",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        420,
        300
      ],
      "parameters": {
        "functionCode": "const input = items[0].json || {};\nconst params = input.params || {};\nconst tool = input.tool || {};\nconst normalized = {\n  runId: input.runId || 'run-id',\n  stepId: input.stepId || 'step',\n  tool: {\n    ref: tool.ref || 'openai',\n    provider: 'openai',\n    operation: tool.operation || params.operation || ''\n  },\n  params,\n  context: input.context || {}\n};\nreturn [{ json: normalized }];"
      }
    },
    {
      "id": "node-switch",
      "name": "Dispatch Operation",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [
        660,
        300
      ],
      "parameters": {
        "propertyName": "={{$json.tool.operation}}",
        "dataType": "string",
        "rules": [
          {
            "operation": "equal",
            "value": "chat.classify"
          },
          {
            "operation": "equal",
            "value": "chat.extract"
          },
          {
            "operation": "equal",
            "value": "chat.summarize"
          }
        ],
        "outputData": "inputData"
      }
    },
    {
      "id": "node-action-1",
      "name": "Action chat.classify",
      "type": "n8n-nodes-base.openAi",
      "typeVersion": 1,
      "position": [
        900,
        200
      ],
      "parameters": {
        "resource": "chat",
        "operation": "classify",
        "text": "={{$json.params.text}}",
        "labels": "={{$json.params.labels}}"
      }
    },
    {
      "id": "node-action-2",
      "name": "Action chat.extract",
      "type": "n8n-nodes-base.openAi",
      "typeVersion": 1,
      "position": [
        900,
        320
      ],
      "parameters": {
        "resource": "chat",
        "operation": "extract",
        "text": "={{$json.params.text}}",
        "schema": "={{$json.params.schema}}"
      }
    },
    {
      "id": "node-action-3",
      "name": "Action chat.summarize",
      "type": "n8n-nodes-base.openAi",
      "typeVersion": 1,
      "position": [
        900,
        440
      ],
      "parameters": {
        "resource": "chat",
        "operation": "summarize",
        "text": "={{$json.params.text}}"
      }
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "Normalize Input",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Input": {
      "main": [
        [
          {
            "node": "Dispatch Operation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Dispatch Operation": {
      "main": [
        [
          {
            "node": "Action chat.classify",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Action chat.extract",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Action chat.summarize",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false,
  "settings": {
    "executionOrder": "v1"
  }
}
