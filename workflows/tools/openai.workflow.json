{
  "name": "so.tool.openai",
  "nodes": [
    {
      "id": "manual",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    },
    {
      "id": "normalize",
      "name": "Normalize Input",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        420,
        300
      ],
      "parameters": {
        "language": "JavaScript",
        "mode": "runOnceForAllItems",
        "jsCode": "return items.map((item) => {\n  const input = item.json || {};\n  const tool = input.tool || {};\n  const params = input.params ?? tool.params ?? {};\n  const operation = input.operation ?? tool.operation ?? params.action ?? input.action;\n  return {\n    json: {\n      provider: 'openai',\n      operation,\n      params\n    }\n  };\n});\n"
      }
    },
    {
      "id": "dispatch",
      "name": "Dispatch Operation",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        660,
        300
      ],
      "parameters": {
        "language": "JavaScript",
        "mode": "runOnceForAllItems",
        "numberOutputs": 4,
        "jsCode": "const routes = {\n  \"text.classifyTextForViolations\": 0,\n  \"text.generateModelResponse\": 1,\n  \"text.generateChatCompletion\": 2,\n  \"sampleFetch\": 3\n};\nconst outputs = Array.from({ length: 4 }, () => []);\nfor (const item of items) {\n  const op = item.json?.operation;\n  const index = routes[op];\n  if (index !== undefined) {\n    outputs[index].push(item);\n  }\n}\nreturn outputs;\n"
      }
    },
    {
      "id": "assistant-classify",
      "name": "text.classifyTextForViolations",
      "type": "n8n-nodes-langchain.openai",
      "typeVersion": 1,
      "position": [
        920,
        150
      ],
      "parameters": {
        "resource": "text",
        "operation": "classifyTextForViolations",
        "text": "={{'Labels: ' + (($json.params.labels || []).join(', ')) + '\\nText: ' + ($json.params.text || '')}}"
      }
    },
    {
      "id": "assistant-extract",
      "name": "text.generateModelResponse",
      "type": "n8n-nodes-langchain.openai",
      "typeVersion": 1,
      "position": [
        920,
        300
      ],
      "parameters": {
        "resource": "text",
        "operation": "generateModelResponse",
        "text": "={{'Schema: ' + ($json.params.schema || '') + '\\nText: ' + ($json.params.text || '')}}"
      }
    },
    {
      "id": "assistant-summarize",
      "name": "text.generateChatCompletion",
      "type": "n8n-nodes-langchain.openai",
      "typeVersion": 1,
      "position": [
        920,
        450
      ],
      "parameters": {
        "resource": "text",
        "operation": "generateChatCompletion",
        "text": "={{$json.params.text}}"
      }
    },
    {
      "id": "sample-fetch",
      "name": "sampleFetch",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        920,
        600
      ],
      "parameters": {
        "language": "JavaScript",
        "mode": "runOnceForAllItems",
        "jsCode": "return items.map((item) => {\n  const input = item.json || {};\n  return {\n    json: {\n      ok: true,\n      data: { sample: true, provider: 'openai', operation: 'sampleFetch', params: input.params || {} },\n      error: null\n    }\n  };\n});\n"
      }
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "Normalize Input",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Input": {
      "main": [
        [
          {
            "node": "Dispatch Operation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Dispatch Operation": {
      "main": [
        {
          "node": "text.classifyTextForViolations",
          "type": "main",
          "index": 0
        },
        {
          "node": "text.generateModelResponse",
          "type": "main",
          "index": 0
        },
        {
          "node": "text.generateChatCompletion",
          "type": "main",
          "index": 0
        },
        {
          "node": "sampleFetch",
          "type": "main",
          "index": 0
        }
      ]
    },
    "text.classifyTextForViolations": {
      "main": []
    },
    "text.generateModelResponse": {
      "main": []
    },
    "text.generateChatCompletion": {
      "main": []
    },
    "sampleFetch": {
      "main": []
    }
  },
  "settings": {},
