{
  "name": "so.tool.openai",
  "nodes": [
    {
      "id": "manual",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    },
    {
      "id": "normalize",
      "name": "Normalize Input",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [
        420,
        300
      ],
      "parameters": {
        "keepOnlySet": true,
        "values": {
          "string": [
            {
              "name": "provider",
              "value": "openai"
            },
            {
              "name": "operation",
              "value": "={{$json.operation}}"
            }
          ],
          "json": [
            {
              "name": "params",
              "value": "={{$json.params}}"
            }
          ]
        }
      }
    },
    {
      "id": "dispatch",
      "name": "Dispatch Operation",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [
        660,
        300
      ],
      "parameters": {
        "propertyName": "={{$json.operation}}",
        "dataType": "string",
        "outputData": "inputData",
        "rules": [
          {
            "operation": "equal",
            "value": "text.classifyTextForViolations"
          },
          {
            "operation": "equal",
            "value": "text.generateModelResponse"
          },
          {
            "operation": "equal",
            "value": "text.generateChatCompletion"
          },
          {
            "operation": "equal",
            "value": "sampleFetch"
          }
        ]
      }
    },
    {
      "id": "assistant-classify",
      "name": "text.classifyTextForViolations",
      "type": "n8n-nodes-langchain.openai",
      "typeVersion": 1,
      "position": [
        920,
        150
      ],
      "parameters": {
        "resource": "text",
        "operation": "classifyTextForViolations",
        "text": "={{'Labels: ' + (($json.params.labels || []).join(', ')) + '\\nText: ' + ($json.params.text || '')}}"
      }
    },
    {
      "id": "assistant-extract",
      "name": "text.generateModelResponse",
      "type": "n8n-nodes-langchain.openai",
      "typeVersion": 1,
      "position": [
        920,
        300
      ],
      "parameters": {
        "resource": "text",
        "operation": "generateModelResponse",
        "text": "={{'Schema: ' + ($json.params.schema || '') + '\\nText: ' + ($json.params.text || '')}}"
      }
    },
    {
      "id": "assistant-summarize",
      "name": "text.generateChatCompletion",
      "type": "n8n-nodes-langchain.openai",
      "typeVersion": 1,
      "position": [
        920,
        450
      ],
      "parameters": {
        "resource": "text",
        "operation": "generateChatCompletion",
        "text": "={{$json.params.text}}"
      }
    },
    {
      "id": "sample-fetch",
      "name": "sampleFetch",
      "typeVersion": 1,
      "position": [
        920,
        600
      ],
      "type": "n8n-nodes-base.set",
      "parameters": {
        "keepOnlySet": true,
        "values": {
          "json": [
            {
              "name": "sample_payload",
              "value": "={{$json.params.sample_payload}}"
            }
          ]
        },
        "text": "={{$json.params.text}}"
      }
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "Normalize Input",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Input": {
      "main": [
        [
          {
            "node": "Dispatch Operation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Dispatch Operation": {
      "main": [
        {
          "node": "text.classifyTextForViolations",
          "type": "main",
          "index": 0
        },
        {
          "node": "text.generateModelResponse",
          "type": "main",
          "index": 0
        },
        {
          "node": "text.generateChatCompletion",
          "type": "main",
          "index": 0
        },
        {
          "node": "sampleFetch",
          "type": "main",
          "index": 0
        }
      ]
    },
    "text.classifyTextForViolations": {
      "main": []
    },
    "text.generateModelResponse": {
      "main": []
    },
    "text.generateChatCompletion": {
      "main": []
    },
    "sampleFetch": {
      "main": []
    }
  },
  "settings": {},
  "pinData": {}
}
