{
  "name": "google-docs tool workflow",
  "nodes": [
    {
      "id": "manual",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    },
    {
      "id": "normalize",
      "name": "Normalize Input",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [
        420,
        300
      ],
      "parameters": {
        "keepOnlySet": true,
        "values": {
          "string": [
            {
              "name": "provider",
              "value": "google-docs"
            },
            {
              "name": "operation",
              "value": "={{$json.operation}}"
            }
          ],
          "json": [
            {
              "name": "params",
              "value": "={{$json.params}}"
            }
          ]
        }
      }
    },
    {
      "id": "dispatch",
      "name": "Dispatch Operation",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [
        660,
        300
      ],
      "parameters": {
        "propertyName": "={{$json.operation}}",
        "dataType": "string",
        "outputData": "inputData",
        "rules": [
          {
            "operation": "equal",
            "value": "document.create"
          },
          {
            "operation": "equal",
            "value": "document.get"
          },
          {
            "operation": "equal",
            "value": "document.update"
          }
        ]
      }
    },
    {
      "id": "document-create",
      "name": "document.create",
      "type": "n8n-nodes-base.googleDocs",
      "typeVersion": 1,
      "position": [
        920,
        150
      ],
      "parameters": {
        "resource": "document",
        "operation": "create",
        "additionalFields": {}
      }
    },
    {
      "id": "document-get",
      "name": "document.get",
      "type": "n8n-nodes-base.googleDocs",
      "typeVersion": 1,
      "position": [
        920,
        300
      ],
      "parameters": {
        "resource": "document",
        "operation": "get",
        "additionalFields": {}
      }
    },
    {
      "id": "document-update",
      "name": "document.update",
      "type": "n8n-nodes-base.googleDocs",
      "typeVersion": 1,
      "position": [
        920,
        450
      ],
      "parameters": {
        "resource": "document",
        "operation": "update",
        "additionalFields": {}
      }
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "Normalize Input",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Input": {
      "main": [
        [
          {
            "node": "Dispatch Operation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Dispatch Operation": {
      "main": [
        {
          "node": "document.create",
          "type": "main",
          "index": 0
        },
        {
          "node": "document.get",
          "type": "main",
          "index": 0
        },
        {
          "node": "document.update",
          "type": "main",
          "index": 0
        }
      ]
    },
    "document.create": {
      "main": []
    },
    "document.get": {
      "main": []
    },
    "document.update": {
      "main": []
    }
  },
  "settings": {},
  "pinData": {}
}
