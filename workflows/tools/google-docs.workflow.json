{
  "name": "google-docs tool workflow",
  "nodes": [
    {
      "id": "manual",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    },
    {
      "id": "normalize",
      "name": "Normalize Input",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        420,
        300
      ],
      "parameters": {
        "jsCode": "return items.map((item) => {\n  const input = item.json || {};\n  const tool = input.tool || {};\n  const params = input.params ?? tool.params ?? {};\n  const operation = input.operation ?? tool.operation ?? params.action ?? input.action;\n  return {\n    json: {\n      provider: 'google-docs',\n      operation,\n      params\n    }\n  };\n});\n"
      }
    },
    {
      "id": "document-create",
      "name": "document.create",
      "type": "n8n-nodes-base.googleDocs",
      "typeVersion": 1,
      "position": [
        920,
        150
      ],
      "parameters": {
        "resource": "document",
        "operation": "create",
        "title": "={{$json.params.title}}",
        "additionalFields": {}
      }
    },
    {
      "id": "document-update",
      "name": "document.update",
      "type": "n8n-nodes-base.googleDocs",
      "typeVersion": 1,
      "position": [
        920,
        300
      ],
      "parameters": {
        "resource": "document",
        "operation": "update",
        "documentId": "={{$json.params.documentId}}",
        "additionalFields": {}
      }
    },
    {
      "id": "document-get",
      "name": "document.get",
      "type": "n8n-nodes-base.googleDocs",
      "typeVersion": 1,
      "position": [
        920,
        450
      ],
      "parameters": {
        "resource": "document",
        "operation": "get",
        "documentId": "={{$json.params.documentId}}",
        "additionalFields": {}
      }
    },
    {
      "id": "sample-fetch",
      "name": "sampleFetch",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        920,
        600
      ],
      "parameters": {
        "jsCode": "return items.map((item) => {\n  const input = item.json || {};\n  return {\n    json: {\n      ok: true,\n      data: { sample: true, provider: 'google-docs', operation: 'sampleFetch', params: input.params || {} },\n      error: null\n    }\n  };\n});\n"
      }
    },
    {
      "id": "dispatch",
      "name": "Dispatch Operation",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        660,
        300
      ],
      "parameters": {
        "numberOutputs": 4,
        "jsCode": "const routes = {\n  \"document.create\": 0,\n  \"document.update\": 1,\n  \"document.get\": 2,\n  \"sampleFetch\": 3\n};\n/** @type {Array<Array<import('n8n-workflow').INodeExecutionData>>} */\nconst outputs = Array.from({ length: 4 }, () => []);\nfor (const item of items) {\n  const op = item.json?.operation;\n  const index = routes[op];\n  if (index !== undefined) {\n    outputs[index].push(item);\n  }\n}\nreturn outputs;\n"
      }
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "Normalize Input",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Input": {
      "main": [
        [
          {
            "node": "Dispatch Operation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Dispatch Operation": {
      "main": [
        [
          {
            "node": "document.create",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "document.update",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "document.get",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "sampleFetch",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "document.create": {
      "main": []
    },
    "document.update": {
      "main": []
    },
    "document.get": {
      "main": []
    },
    "sampleFetch": {
      "main": []
    }
  },
  "settings": {},
  "pinData": {}
}
