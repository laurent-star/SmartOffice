{
  "name": "tool-google-calendar",
  "nodes": [
    {
      "id": "node-trigger",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    },
    {
      "id": "node-normalize",
      "name": "Normalize Input",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        420,
        300
      ],
      "parameters": {
        "functionCode": "const input = items[0].json || {};\nconst params = input.params || {};\nconst tool = input.tool || {};\nconst normalized = {\n  runId: input.runId || 'run-id',\n  stepId: input.stepId || 'step',\n  tool: {\n    ref: tool.ref || 'google-calendar',\n    provider: 'google-calendar',\n    operation: tool.operation || params.operation || ''\n  },\n  params,\n  context: input.context || {}\n};\nreturn [{ json: normalized }];"
      }
    },
    {
      "id": "node-switch",
      "name": "Dispatch Operation",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [
        660,
        300
      ],
      "parameters": {
        "propertyName": "={{$json.tool.operation}}",
        "dataType": "string",
        "rules": [
          {
            "operation": "equal",
            "value": "event.create"
          },
          {
            "operation": "equal",
            "value": "event.getAll"
          }
        ],
        "outputData": "inputData"
      }
    },
    {
      "id": "node-action-1",
      "name": "Action event.create",
      "type": "n8n-nodes-base.googleCalendar",
      "typeVersion": 1,
      "position": [
        900,
        200
      ],
      "parameters": {
        "resource": "event",
        "operation": "create",
        "summary": "={{$json.params.summary}}",
        "start": "={{$json.params.start}}",
        "end": "={{$json.params.end}}"
      }
    },
    {
      "id": "node-action-2",
      "name": "Action event.getAll",
      "type": "n8n-nodes-base.googleCalendar",
      "typeVersion": 1,
      "position": [
        900,
        320
      ],
      "parameters": {
        "resource": "event",
        "operation": "getAll",
        "timeMin": "={{$json.params.timeMin}}",
        "timeMax": "={{$json.params.timeMax}}"
      }
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "Normalize Input",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Input": {
      "main": [
        [
          {
            "node": "Dispatch Operation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Dispatch Operation": {
      "main": [
        [
          {
            "node": "Action event.create",
            "type": "main",
            "index": 0
          }
        ],
        [
          {
            "node": "Action event.getAll",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false,
  "settings": {
    "executionOrder": "v1"
  }
}
