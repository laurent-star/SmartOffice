{
  "name": "so.tool.google-drive",
  "nodes": [
    {
      "id": "manual",
      "name": "Manual Trigger",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    },
    {
      "id": "normalize",
      "name": "Normalize Input",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [
        420,
        300
      ],
      "parameters": {
        "keepOnlySet": true,
        "values": {
          "string": [
            {
              "name": "provider",
              "value": "google-drive"
            },
            {
              "name": "operation",
              "value": "={{$json.operation}}"
            }
          ],
          "json": [
            {
              "name": "params",
              "value": "={{$json.params}}"
            }
          ]
        }
      }
    },
    {
      "id": "dispatch",
      "name": "Dispatch Operation",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [
        660,
        300
      ],
      "parameters": {
        "propertyName": "={{$json.operation}}",
        "dataType": "string",
        "outputData": "inputData",
        "rules": [
          {
            "operation": "equal",
            "value": "file.download"
          },
          {
            "operation": "equal",
            "value": "file.upload"
          },
          {
            "operation": "equal",
            "value": "fileFolder.search"
          },
          {
            "operation": "equal",
            "value": "folder.create"
          },
          {
            "operation": "equal",
            "value": "sampleFetch"
          }
        ]
      }
    },
    {
      "id": "file-download",
      "name": "file.download",
      "type": "n8n-nodes-base.googleDrive",
      "typeVersion": 1,
      "position": [
        920,
        150
      ],
      "parameters": {
        "resource": "file",
        "operation": "download",
        "fileId": "={{$json.params.fileId}}",
        "options": {
          "binaryPropertyName": "={{$json.params.binaryPropertyName || 'data'}}"
        }
      }
    },
    {
      "id": "file-upload",
      "name": "file.upload",
      "type": "n8n-nodes-base.googleDrive",
      "typeVersion": 1,
      "position": [
        920,
        450
      ],
      "parameters": {
        "resource": "file",
        "operation": "upload",
        "binary": "={{$json.params.binary || 'data'}}",
        "folderId": "={{$json.params.folderId}}",
        "name": "={{$json.params.name}}",
        "options": {
          "mimeType": "={{$json.params.mimeType}}"
        }
      }
    },
    {
      "id": "fileFolder-search",
      "name": "fileFolder.search",
      "type": "n8n-nodes-base.googleDrive",
      "typeVersion": 1,
      "position": [
        920,
        600
      ],
      "parameters": {
        "resource": "fileFolder",
        "operation": "search",
        "query": "={{$json.params.query}}",
        "options": {
          "limit": "={{$json.params.limit}}",
          "returnAll": "={{$json.params.returnAll}}"
        }
      }
    },
    {
      "id": "folder-create",
      "name": "folder.create",
      "type": "n8n-nodes-base.googleDrive",
      "typeVersion": 1,
      "position": [
        920,
        750
      ],
      "parameters": {
        "resource": "folder",
        "operation": "create",
        "name": "={{$json.params.name}}",
        "options": {
          "parentFolderId": "={{$json.params.parentFolderId}}"
        }
      }
    },
    {
      "id": "sample-fetch",
      "name": "sampleFetch",
      "typeVersion": 1,
      "position": [
        920,
        900
      ],
      "type": "n8n-nodes-base.googleDrive",
      "parameters": {
        "resource": "file",
        "operation": "download",
        "fileId": "={{$json.params.fileId}}",
        "options": {
          "binaryPropertyName": "={{$json.params.binaryPropertyName || 'data'}}"
        }
      }
    }
  ],
  "connections": {
    "Manual Trigger": {
      "main": [
        [
          {
            "node": "Normalize Input",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Normalize Input": {
      "main": [
        [
          {
            "node": "Dispatch Operation",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Dispatch Operation": {
      "main": [
        {
          "node": "file.download",
          "type": "main",
          "index": 0
        },
        {
          "node": "file.upload",
          "type": "main",
          "index": 0
        },
        {
          "node": "fileFolder.search",
          "type": "main",
          "index": 0
        },
        {
          "node": "folder.create",
          "type": "main",
          "index": 0
        },
        {
          "node": "sampleFetch",
          "type": "main",
          "index": 0
        }
      ]
    },
    "file.download": {
      "main": []
    },
    "file.upload": {
      "main": []
    },
    "fileFolder.search": {
      "main": []
    },
    "folder.create": {
      "main": []
    },
    "sampleFetch": {
      "main": []
    }
  },
  "settings": {},
  "pinData": {}
}
