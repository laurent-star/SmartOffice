{
  "id": "google-calendar",
  "version": "1.0.0",
  "description": "Gestion des evenements calendrier via Google Calendar.",
  "actions": [
    {
      "name": "event.create",
      "input": [
        "summary",
        "start",
        "end"
      ],
      "output": "eventId"
    },
    {
      "name": "event.getAll",
      "input": [
        "timeMin",
        "timeMax"
      ],
      "output": "events"
    }
  ],
  "categories": [
    "calendar"
  ]
}
